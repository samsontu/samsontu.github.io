<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE rdf:RDF [
	 <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
	 <!ENTITY a 'http://protege.stanford.edu/system#'>
	 <!ENTITY kb 'http://protege.stanford.edu/kb#'>
	 <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
]>
<rdf:RDF xmlns:rdf="&rdf;"
	 xmlns:a="&a;"
	 xmlns:kb="&kb;"
	 xmlns:rdfs="&rdfs;">
<a:_pal_constraint rdf:about="&kb;Recommendations_Class1"
	 a:_pal_name="all slots used"
	 rdfs:label="Recommendations_Class1">
	<a:_pal_range>(defrange ?slot :FRAME :STANDARD-SLOT)
(defrange ?class :FRAME :STANDARD-CLASS)</a:_pal_range>
	<a:_pal_statement>(forall ?slot 
        (exists ?class 
                (template-slot-of ?slot ?class)))</a:_pal_statement>
</a:_pal_constraint>
</rdf:RDF>
